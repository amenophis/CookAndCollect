{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "^1.0@dev",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "2.2.x-dev",
        "doctrine/doctrine-migrations-bundle": "9999999-dev",
        "doctrine/orm": "9999999-dev",
        "phpdocumentor/reflection-docblock": "5.x-dev",
        "ramsey/uuid": "4.x-dev",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/mime": "5.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.2.*",
        "symfony/process": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/string": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-bundle": "^5.2@beta",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/webpack-encore-bundle": "9999999-dev",
        "symfony/yaml": "5.2.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "jangregor/phpstan-prophecy": "9999999-dev",
        "phpstan/extension-installer": "9999999-dev",
        "phpstan/phpstan": "0.12.x-dev",
        "phpstan/phpstan-doctrine": "0.12.x-dev",
        "phpstan/phpstan-symfony": "0.12.x-dev",
        "phpunit/phpunit": "9.5.x-dev",
        "symfony/browser-kit": "^5.2@beta",
        "symfony/css-selector": "^5.2@beta",
        "symfony/debug-bundle": "^5.2@beta",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "9999999-dev",
        "symfony/stopwatch": "^5.2@beta",
        "symfony/var-dumper": "^5.2@beta",
        "symfony/web-profiler-bundle": "^5.2@beta"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
